
<h1>Prueba de busqueda de laboratorio <%= @codigo %></h1>
<% if @laboratory1.size != 0 %>
<table class="table">
<table class="table">
  <tr>
    <th>Dia</th>
    <th>Inicio</th>
    <th>Fin</th>
    <th>Curso</th>
    <th>Profesor</th>
    
    <th></th>
    <th></th>
    <th></th>
  </tr>
  <% @laboratory1.each do |laboratory| %>
  <tr>
    <td><%= laboratory.day_id %></td>
    <td><%= laboratory.start %></td>
    <td><%= laboratory.end %></td>
    <td><%= laboratory.title %></td>
    <td><%= laboratory.body%><%= laboratory.lastname%></td>
    <td><%= link_to 'Ver', laboratory %></td>
    <% if can? :update, @laboratories %>
        <td><%= link_to 'Editar', edit_laboratory_path(laboratory) %></td>
    <% end %>
    <% if can? :destroy, @laboratories %>
        <td><%= link_to 'Eliminar', laboratory, method: :delete, data: { confirm: '¿Está seguro?' } %></td>
    <% end %>  
  </tr>
<% end %>

  </table>
  <% content_for :head do %>
<%= javascript_tag "var laboratories = #{@lab};"%>
<%= javascript_include_tag 'search' %>

<% end %>
<div id='calendar'></div>
<div id="event_edit_container">
	<form>
		<input type="hidden" />
		<ul>
			<li>
				<span>Date: </span><span class="date_holder"></span> 
			</li>
			<li>
				<label for="start">Start Time: </label><span class="start1"></span>
			</li>
			<li>
				<label for="end">End Time: </label><span class="end1"></span>
			</li>
			<li>
				<label for="title">Curso: </label><span class="title1"></span>
			</li>
			<li>
				<label for="body">Docente: </label><span class="body1"></span>
			</li>
		</ul>
	</form>
</div>
<% else %>
	
	<h2> No hay horarios asignados</h2>
	
<% end %>

