<h1>Mantenimiento de Laboratorios</h1>

<h2>Listado</h2>
<table class="table">
  <tr>
    <th>Number</th>
    <th>Capacity</th>
    <th>Floor</th>
    <th>Numcomputers</th>
    <th>Airconditioning</th>
    <th>Projector</th>
    <th>Sound</th>
    <th colspan="3"></th>
  </tr>

<% @laboratories.each do |laboratory| %>
  <tr>
    <td><%= laboratory.number %></td>
    <td><%= laboratory.capacity %></td>
    <td><%= laboratory.floor %></td>
    <td><%= laboratory.numComputers %></td>
    <td><%= laboratory.airConditioning %></td>
    <td><%= laboratory.projector %></td>
    <td><%= laboratory.sound %></td>
    <td><%= link_to 'Ver', laboratory %></td>
    <td><%= link_to 'Editar', edit_laboratory_path(laboratory) %></td>
    <td><%= link_to 'Eliminar', laboratory, method: :delete, data: { confirm: '¿Está seguro?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nuevo Laboratorio', new_laboratory_path, :class => 'btn btn-primary' %>
<br />